{
    "name": "origami/upload",
    "description": "Laravel frontend upload helper package",
    "license": "MIT",
    "authors": [
        {
            "name": "Papertank",
            "email": "david@papertank.co.uk"
        }
    ],
    "require": {
        "php": "^7.2|^8.0.2",
        "illuminate/cache": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/console": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "intervention/image": "2.*",
        "spatie/image": "^2.2"
    },
    "suggest": {
        "spatie/laravel-medialibrary": "Associate uploaded files with Eloquent models"
    },
    "autoload": {
        "psr-4": {
            "Origami\\Upload\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "Origami\\Upload\\UploadServiceProvider"
            ],
            "aliases": {
                "Upload": "Origami\\Upload\\UploadFacade"
            }
        }
    },
    "minimum-stability": "stable"
}
